<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="config/database.properties"></properties>
	<typeAliases>
		<typeAlias type="com.project.user.UserVO" alias="users"></typeAlias>
		<typeAlias type="com.project.intro.IntroVO" alias="intro"></typeAlias>
		<typeAlias type="com.project.review.ReviewVO" alias="review"></typeAlias>
		<typeAlias type="com.project.mypage.MypageVO" alias="mypage"></typeAlias>
		<typeAlias type="com.project.subscribe.SubscribeVO" alias="subscribe"></typeAlias>
		<typeAlias type="com.project.mainmenu.MenuVO" alias="menu"></typeAlias>
	</typeAliases>

	<mappers>

		<mapper resource="mappings/subscribe-mapping.xml" />
		<mapper resource="mappings/mypage-mapping.xml" />
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/intro-mapping.xml" />
		<mapper resource="mappings/board-mapping.xml" />
		<mapper resource="mappings/menu-mapping.xml" />

	</mappers>
</configuration>
